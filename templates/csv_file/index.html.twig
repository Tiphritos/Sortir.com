{% extends 'base.html.twig' %}

{% block title %}CsvFile index{% endblock %}

{% block body %}
    <h1>CsvFile index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Participantfile</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for csv_file in csv_files %}
            <tr>
                <td>{{ csv_file.id }}</td>
                <td>{{ csv_file.participantfile }}</td>
                <td>
                    <a href="{{ path('app_csv_file_show', {'id': csv_file.id}) }}">show</a>
                    <a href="{{ path('app_csv_file_edit', {'id': csv_file.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_csv_file_new') }}">Create new</a>
{% endblock %}
